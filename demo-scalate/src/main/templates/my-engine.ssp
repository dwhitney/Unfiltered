#import(unfiltered.demo.User)
<%@ val user: User %>
<%@ val quote: String %>
<h1>${quote}</h1>
<table>
    <tr>
        <td>username:</td>
        <td>${user.username}</td>
    </tr>
    <tr>
        <td>first name:</td>
        <td>${user.firstName}</td>
